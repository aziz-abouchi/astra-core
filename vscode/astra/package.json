{
  "name": "astra-syntax",
  "displayName": "Astra Syntax (HM)",
  "version": "0.4.1",
  "publisher": "astra",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "astra",
        "aliases": [
          "Astra"
        ],
        "extensions": [
          ".astra"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "astra",
        "scopeName": "source.astra",
        "path": "./syntaxes/astra.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "astra",
        "path": "./snippets/astra.json"
      }
    ]
  }
}